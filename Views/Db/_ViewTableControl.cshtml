@model efcore1.Models.TableProvider

<form asp-controller="Db" asp-action="ShowTable" method="get">
	<!-- <select asp-items="@(new SelectList(Model.AvailableTables, "Value", "Key"))" name="__entityType"> -->
	<select asp-items="@(new SelectList(Model.AvailableTables.ToDictionary(n => n.Name, n => n.FullName), "Value", "Key"))" name="__entityType">
		<option value="@Model.EntityType.FullName">@Model.EntityType.Name</option>
	</select>
	<button type="submit">Show</button>
</form>

<h2>@ViewData["Title"]</h2>
<h3 class="error-message">@ViewData["Message"]</h3>

<partial name="../Db/_ViewTable.cshtml" />
